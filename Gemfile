# A sample Gemfile
source "https://rubygems.org"

gem "jekyll"
# The git pin is necessary so we can disable `svgo`, which is noising up the build process:
# https://github.com/valerijaspasojevic/jekyll-compress-images/issues/4
gem 'jekyll-compress-images', :git => 'https://github.com/valerijaspasojevic/jekyll-compress-images.git'
gem 'jekyll-seo-tag'
gem 'jekyll-toc'
gem 'jekyll-last-modified-at'
gem "airtable"
gem "activesupport"
